<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009"
        xmlns:s="library://ns.adobe.com/flex/spark"
        alpha.disabled="0.5">

    <fx:Metadata>
        [HostComponent("spark.components.Button")]
    </fx:Metadata>

    <s:states>
        <s:State name="disabled"/>
        <s:State name="down"/>
        <s:State name="over"/>
        <s:State name="up"/>
    </s:states>

    <s:BitmapImage visible="true" visible.down="false">
        <s:source>
            <s:MultiDPIBitmapSource source160dpi="assets/low-res/gpsA.png"
                                    source240dpi="assets/med-res/gpsA.png"
                                    source320dpi="assets/high-res/gpsA.png"/>
        </s:source>
    </s:BitmapImage>

    <s:BitmapImage visible="false" visible.down="true">
        <s:source>
            <s:MultiDPIBitmapSource source160dpi="assets/low-res/gpsB.png"
                                    source240dpi="assets/med-res/gpsB.png"
                                    source320dpi="assets/high-res/gpsB.png"/>
        </s:source>
    </s:BitmapImage>

</s:Skin>
